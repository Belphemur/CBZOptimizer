  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch file",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${file}"
    },
    {
      "name": "Optimize Testdata",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/cbzoptimizer",
      "args": ["optimize", "${workspaceFolder}/testdata", "-l", "debug"],
      "cwd": "${workspaceFolder}"
    }
  ]
}
